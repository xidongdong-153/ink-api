{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "src",
    "compilerOptions": {
        "assets": ["assets/**/*"],
        "watchAssets": true,
        "deleteOutDir": true,
        "builder": "swc",
        "typeCheck": true
    }
}
